<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MusicLoadingView">
        <attr name="loading_min_width" format="dimension" />
        <attr name="loading_update_width" format="dimension" />
        <attr name="loading_update_time" format="integer" />
        <attr name="loading_bg_color" format="color" />
        <attr name="loading_direction" format="enum">
            <enum name="center_to_both_sides" value="0"/>
            <enum name="both_sides_to_center" value="1"/>
            <enum name="left_to_right" value="2"/>
            <enum name="right_to_left" value="3"/>
        </attr>
    </declare-styleable>


</resources>